// The UI theme: light, mid, dark, darkest
type(fltk_viewer_window):theme=light

// Data
// - dataset to load
type(tubes):datapath=""
// - initial glyph layer config
//type(tubes):layer_config_file="./testdata/layerconf_4layers_interpolationdemo.xml"

// Visualization and general settings
// - Grid mode: 0 - none, 1 - color only, 2 - normals only, 3 - color and normals
type(tubes):grid_mode=0
type(tubes):show_hidden_glyphs=false
// - Voxelization device
type(tubes):voxelize_gpu=false

// Framework settings (make persistent in your local .cfg)
type(tubes):instant_redraw_proxy=false
type(tubes):fix_view_up_dir_proxy=false
