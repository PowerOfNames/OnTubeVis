<GlyphConfiguration>
    <ColorMaps/>
    <Layers>
        <Layer name="Pressure (color)" active="true" glyph="color" sampling="original">
            <Property name="interpolate" value="1.000000"/>
            <Property name="color" attrib_name="|p|" color_map_name="imola" in_range="0 10000" out_range="0 1"/>
        </Layer>
        <Layer name="Velocity (lineplot)" active="true" glyph="line_plot" sampling="original">
            <Property name="outline" value="0.030000"/>
            <Property name="interpolate" value="1.000000"/>
            <Property name="color_0" color="1 1 1" out_range="0 1"/>
            <Property name="value_0" attrib_name="Velocity" in_range="0.25 3.459" out_range="0 0.54"/>
            <Property name="color_1" color="0 0 0" out_range="0 1"/>
            <Property name="value_1" in_range="0 1" out_range="0 1"/>
            <Property name="color_2" color="0 0 0" out_range="0 1"/>
            <Property name="value_2" in_range="0 1" out_range="0 1"/>
            <Property name="color_3" color="0 0 0" out_range="0 1"/>
            <Property name="value_3" in_range="0 1" out_range="0 1"/>
        </Layer>
        <Layer name="Angular Vel. (sign)" active="true" glyph="sign_blob" sampling="original">
            <Property name="outline" value="0.020000"/>
            <Property name="size" value="1.000000" out_range="0 1"/>
            <Property name="color" attrib_name="AngularVelocity" color_map_name="vik" in_range="-50 50" out_range="0 1"/>
            <Property name="value" attrib_name="AngularVelocity" in_range="-50 50"/>
        </Layer>
    </Layers>
</GlyphConfiguration>
